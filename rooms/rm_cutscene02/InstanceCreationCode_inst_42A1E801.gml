strings[7] = "Bem, eu sei de alguém que pode nos ajudar";
strings[8] = "Excelente, e quem é?";
strings[9] = "Tem um professor que pode nos ajudar, ele geralmente está lá pelo Lab de química. Podemos nos encontrar com ele as 2:00 pm.";
